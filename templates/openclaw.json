{
  "$schema": "https://docs.openclaw.ai/schema/openclaw.json",
  "agents": {
    "defaults": {
      "model": {
        "primary": "blockrun/auto",
        "fallbacks": ["blockrun/eco"]
      },
      "heartbeat": {
        "every": "0m"
      }
    },
    "list": [
      {
        "id": "ceo",
        "workspace": "{{PROJECT_DIR}}",
        "heartbeat": {
          "every": "1h"
        }
      },
      {
        "id": "scout",
        "workspace": "{{PROJECT_DIR}}/agents/scout"
      },
      {
        "id": "pm",
        "workspace": "{{PROJECT_DIR}}/agents/pm"
      },
      {
        "id": "design",
        "workspace": "{{PROJECT_DIR}}/agents/design"
      },
      {
        "id": "engineer",
        "workspace": "{{PROJECT_DIR}}/agents/engineer"
      },
      {
        "id": "marketing",
        "workspace": "{{PROJECT_DIR}}/agents/marketing"
      },
      {
        "id": "ops",
        "workspace": "{{PROJECT_DIR}}/agents/ops"
      }
    ]
  },
  "session": {
    "dmScope": "per-channel-peer"
  }
}
